{
  "name": "@in-ch/isdeepequal",
  "version": "1.0.0",
  "description": "😵‍💫 Deeply compares two values to determine if they are equivalent.",
  "type": "commonjs",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "rollup -c ../../rollup.config.js --bundleConfigAsCjs"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./esm/*.d.mts",
        "default": "./esm/*.mjs"
      },
      "default": {
        "types": "./*.d.ts",
        "default": "./*.js"
      }
    }
  },
  "files": [
    "**"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC"
}
